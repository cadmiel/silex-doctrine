{
    "name": "Silex",
    "description": "Testando silex 2",
    "type": "project",
    "homepage": "https://silex.symfony.com",
    "require": {
        "silex/silex": "~2.0",
        "doctrine/orm": "2.5.*",
        "symfony/var-dumper": "^3.3",
        "doctrine/migrations": "^1.5",
        "twig/twig": "^2.4",
        "fzaninotto/faker": "^1.7"
    },
    "require-dev":{
        "phpunit/phpunit": "^6.3",
        "mockery/mockery": "^0.9.9"
    },
    "autoload":{
        "psr-4":{
            "App\\":"src/App"
        },"files": [
            "src/App/Helper/Util.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "test/"
        }
    },
    "scripts":{
        "test": "vendor/bin/phpunit  --colors=always"
    }
}
